def packageType
def trace 

def execute() 
{
  stage('Build') {
	 trace.startStage('BUILD')

	 try {

      def nugent = load("pipeline/components/_nugent")
      nugent.executeMSBuild(packageType)

      def logstash = load("pipeline/components/_logstash")
      logstash.copyLogFiles()
    }
    catch (err) {
      throw err
    }
    finally {
      trace.endStage('BUILD')
    }
  }
}
return this


